<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Home Page</title>
</head>
<body>

    <form class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">Login</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="text" placeholder="Login">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-2" for="pwd">Hasło:</label>
            <div class="col-sm-10">
                <input type="password" class="form-control" id="pwd" placeholder="Hasło">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button onclick="sendForm()">Zaloguj</button>
            </div>
        </div>
    </form>
    <script src="~/Scripts/OurScripts/LogIn.js"></script>
</body>
</html>